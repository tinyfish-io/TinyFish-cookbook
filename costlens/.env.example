# CostLens environment configuration

# Server (local development only; do not set on Vercel)
# PORT is provided automatically by Vercel.
# CORS_ORIGIN is optional on Vercel if VERCEL_URL is present.
# For custom domains, set CORS_ORIGIN to a comma-separated allowlist.
PORT=3000
CORS_ORIGIN=http://localhost:3000

# TinyFish web agent
TINYFISH_ENDPOINT=https://agent.tinyfish.ai
TINYFISH_API_KEY=your_tinyfish_api_key
TINYFISH_BROWSER_PROFILE=stealth
TINYFISH_PROXY_ENABLED=false
# One of: US, GB, CA, DE, FR, JP, AU
TINYFISH_PROXY_COUNTRY_CODE=US
TINYFISH_REQUEST_TIMEOUT_MS=25000
TINYFISH_SSE_TIMEOUT_MS=130000

# OpenAI
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o

# Investigation limits
INVESTIGATION_TIMEOUT_MS=60000
INVESTIGATION_FAST_MODE=true
